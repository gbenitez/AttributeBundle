{
  "name": "gbenitez/attribute-bundle",
  "version": "2.0",
  "type": "symfony-bundle",
  "description": "Symfony Attribute Bundle para Forma",
  "keywords": ["form", "attribute", "bundle"],
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": "^2.5||^3.0||^4.0||6.1.*"
  },
  "autoload": {
    "psr-4": {
      "gbenitez\\Bundle\\AttributeBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "gbenitez\\Bundle\\AttributeBundle\\Tests\\": "Tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^6.1"
  }
}
