{
  "name": "gbenitez/attribute-bundle",
  "version": "3.0",
  "type": "symfony-bundle",
  "description": "Symfony Attribute Bundle para Form",
  "keywords": [
    "form",
    "attribute",
    "bundle"
  ],
  "require": {
    "php": ">=7.4",
    "ext-gd": "*",
    "symfony/form": "^5.4|^6.0|^7.0",
    "symfony/framework-bundle": "^5.4|^6.0|^7.0",
    "symfony/translation": "^5.4|^6.0|^7.0",
    "twig/twig": "^2.10|^3.0"
  },
  "autoload": {
    "psr-4": {
      "Gbenitez\\AttributeBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gbenitez\\AttributeBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0-dev"
    }
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^5.4|^6.0|^7.0"
  }
}